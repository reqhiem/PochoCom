extends layout


block content
    <div class="container">
        <div class="row">
            <div class="col-lg-10 mx-auto">
                <div class="card-header">
                    form(action="/admin/contribuidores/agregar" method="POST")
                        <div class="form-outline mb-4 row">
                            <div class="col">
                                <label class="col-form-label" for="ActName">Nombre:</label>
                                <input type="text" id="ActName" name="Nombre" class="form-control" />
                            </div>
                        </div>

                        <div class="form-outline mb-4 row">
